{
  "name": "puppet-nsd",
  "version": "1.3.0",
  "author": "Vox Pupuli",
  "summary": "Puppet NSD management module",
  "license": "Apache-2.0",
  "source": "git://github.com/voxpupuli/puppet-nsd.git",
  "project_page": "https://github.com/voxpupuli/puppet-nsd",
  "issues_url": "https://github.com/voxpupuli/puppet-nsd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.3",
        "6.2",
        "6.1"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "12",
        "11"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 8.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.2.1 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 5.2.0 < 7.0.0"
    }
  ]
}
